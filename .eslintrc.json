{
    "extends": [
        "eslint:recommended",
        "prettier",
        "prettier/react"],
        "rules": {
            "react/prop-types": 0,
            "no-console": 1,
            "react-hooks/rules-of-hooks": 2,
            "react-hooks/exhaustive-deps": 1
        },
    "plugins": [],
    "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    }
}